<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <connectionStrings>
    <add name="OracleConnection" connectionString="User Id=DBUSERID; Password=DBPASSWORD; Data Source=DBHOST;" />
  </connectionStrings>
  <appSettings>
    <add key="TopMost" value="false" />
    <add key="DoInitExec" value="false" />
    <add key="LogDays" value="10" />
    <add key="SeparateDir" value="10" />
    <add key="DBHost" value="DHost" />
    <add key="DBUserId" value="UID" />
    <add key="DBPassword" value="PWD" />
  </appSettings>
</configuration>